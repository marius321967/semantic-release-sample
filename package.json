{
  "name": "semantic-release-sample",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo 'tests successful'",
    "prepare": "husky install",
    "release": "yarn semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marius321967/semantic-release-sample.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marius321967/semantic-release-sample/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "homepage": "https://github.com/marius321967/semantic-release-sample#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "husky": "^8.0.2",
    "semantic-release": "^19.0.5"
  },
  "release": {
    "debug": true,
    "branches": [
      "master",
      { "name": "feat/.+", "prerelease": true }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {}
}
